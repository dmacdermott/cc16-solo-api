{
   "name": "soloAPI1",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^8.0.29",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "express": "^4.17.1",
      "nodemon": "^2.0.6",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.29",
      "typeorm-seeding": "^1.6.1",
      "uuid": "^8.3.1"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "makeMigrations": "yarn migration:generate",
      "migrate": "yarn migration:run",
      "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
